{
  "name": "mailgun_senior_code",
  "version": "1.0.0",
  "description": "coding challenge for mailgun senior position",
  "scripts": {
    "test": "jest",
    "test-watch": "jest --watchAll",
    "start": "next start",
    "lint": "npx eslint --ext .ts,.tsx,.json src",
    "lint-fix": "npx eslint --fix --ext .ts,.tsx,.json src",
    "dev": "next dev",
    "build": "next build"
  },
  "devDependencies": {
    "@babel/plugin-syntax-jsx": "^7.12.13",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "babel-jest": "^26.6.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.20.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-html": "^6.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "less": "^4.1.1",
    "prettier": "^2.2.1",
    "react-test-renderer": "^17.0.2",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@reduxjs/toolkit": "^1.5.1",
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "fontsource-roboto": "^4.0.0",
    "jest-cli": "^26.6.3",
    "moment": "^2.29.1",
    "next": "^10.1.3",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.4",
    "recharts": "^2.0.9"
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ],
    "env": {
      "test": {
        "presets": [
          "@babel/preset-env",
          "@babel/react",
          "next/babel"
        ]
      },
      "development": {
        "presets": [
          "next/babel"
        ]
      }
    }
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "airbnb/hooks",
      "prettier"
    ],
    "env": {
      "browser": true,
      "es2020": true,
      "jest": true,
      "node": true
    },
    "ignorePatterns": [
      "_app.jsx"
    ],
    "plugins": [
      "react",
      "html"
    ],
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module",
      "ecmaFeatures": {
        "impliedStrict": true,
        "jsx": true
      }
    },
    "rules": {
      "react/forbid-prop-types": [
        "warn",
        {
          "forbid": [
            "any",
            "array"
          ]
        }
      ],
      "no-underscore-dangle": [
        "error",
        {
          "allow": [
            "_id"
          ]
        }
      ],
      "import/prefer-default-export": "error",
      "no-console": "error",
      "max-len": [
        "error",
        {
          "code": 80
        }
      ],
      "no-use-before-define": [
        "error",
        "nofunc"
      ]
    }
  },
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonkruse00/USAA_TEST_ENV_2.git"
  },
  "author": "jon kruse",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jonkruse00/USAA_TEST_ENV_2/issues"
  },
  "homepage": "https://github.com/jonkruse00/USAA_TEST_ENV_2#readme"
}
